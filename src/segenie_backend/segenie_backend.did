type Portal = record {
    "id": nat;
    "name": text;
    "description": text;
    "image_url": opt text;
    "creator": principal;
}

service : {
    "create_portal": (text, text) -> (text);
    "update_portal_metadata": (nat, text, text, opt text) -> (text);
    "get_portal": (nat) -> (Portal);
}
